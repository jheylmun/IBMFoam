FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);
freestreamValue uniform (0.3 0 0);
boundaryField
{
    inlet
    {
        type            cyclic;
        value           $freestreamValue;
    }

    outlet
    {
        type            cyclic;
    }

    freestream1
    {
        type            cyclic;
    }
    freestream2
    {
        type            cyclic;
    }
    freestream3
    {
        type            cyclic;
    }
    freestream4
    {
        type            cyclic;
    }
}

